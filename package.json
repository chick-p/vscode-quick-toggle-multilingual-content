{
	"name": "vscode-quick-multilingual-content",
	"displayName": "vscode-quick-multilingual-content",
	"description": "Switch multilingual contents with shortcut key",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-quick-multilingual-content.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-quick-multilingual-content.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.66.0",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.6.2",
		"typescript": "^4.5.5"
	}
}
